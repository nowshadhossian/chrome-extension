{
  "manifest_version": 2,
  "name": "my-sub-color",
  "description": "Run my color tool",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "tabs", "<all_urls>"
  ],
  "content_scripts": [
    {
      "all_frames": false,
      "matches": ["<all_urls>"],
      "css": [],
      "js": ["jquery.min.js"]
    }
  ]
}
